#:kivy 2.2.0
MDScreen:
    BoxLayout:
        orientation: "vertical"

        MDTopAppBar:
            title: "Schedules"
            left_action_items: [["arrow-left", lambda x: app.root.current_screen.on_back_button()]]

        BoxLayout:
            orientation:'horizontal'
            size_hint_y: None
            height: dp(48)

            Spinner:
                id : year
                text:'Year'
                color: 0,0,0,1
                values:["2022","2023"]
                on_text: root.year_clicked(year.text)
                background_normal:''
                background_color: 1,1,1,1

            Spinner:
                id: month
                text:'Month'
                color: 0,0,0,1
                values:["January","February","March","April","May","June","July","August","September","October","November","December"]
                on_text: root.month_clicked(month.text)
                background_normal:''
                background_color: 1,1,1,1

            
        ScrollView:
            MDList:
                TwoLineIconListItem:
                    text: "Session Ended"
                    secondary_text:'3 PM - 4 PM'
                    ImageLeftWidget: #256x256 png image only!!!!
                        source: "Assets\Days\day1.png"
                TwoLineIconListItem:
                    text: "Upcoming Session"
                    secondary_text: '3 PM - 4 PM'
                    ImageLeftWidget: #256x256 png image only!!!!
                        source: "Assets\Days\day2.png"
